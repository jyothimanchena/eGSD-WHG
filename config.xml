<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eGSD_WHG.eGSD" version="2.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/2.8">
    <name>eGSD-WHG</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="whatsapp:*" />
    <access launch-external="yes" origin="sms:*" />
    <access origin="cdvfile://*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ioss">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <branch-config>
        <android-prefix value="/U09p" />
        <host name="egsdwhg.app.link" scheme="https" />
    </branch-config>
	<branch-config>
    <android-prefix value="/U09p" />
    <host name="bnc.lt" scheme="https"/>
</branch-config>

    
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <engine name="android" spec="^6.2.3" />
    <plugin name="branch-cordova-sdk" spec="^2.6.6">
        <variable name="BRANCH_KEY" value="key_live_gcvqXCr6xFPow2LCtXn3LjpkxsnvdUC9" />
        <variable name="URI_SCHEME" value="eGSD-WHG" />
    </plugin>
    <plugin name="cordova-plugin-compat" spec="^1.0.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <plugin name="cordova-plugin-sdwebimage" spec="^1.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.1.8" />
    <plugin name="cordova.plugins.diagnostic" spec="^3.6.6" />
</widget>
